{
  "root": "tools/mud-cli",
  "sourceRoot": "tools/mud-cli",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o=bin/mud-cli cmd/main.go",
        "cwd": "tools/mud-cli"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run ./cmd/main.go",
        "cwd": "tools/mud-cli"
      }
    }
  }
}
